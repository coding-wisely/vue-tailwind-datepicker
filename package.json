{
  "name": "@coding-wisely/vue-tailwind-datepicker",
  "version": "1.1.5",
  "description": "Datepicker component based on Tailwind.css and Vue.js",
  "main": "dist/vue-tailwind-datepicker.umd.js",
  "module": "dist/vue-tailwind-datepicker.esm.js",
  "unpkg": "dist/vue-tailwind-datepicker.min.js",
  "browser": {
    "./sfc": "src/vue-tailwind-datepicker.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-tailwind-datepicker.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-tailwind-datepicker.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-tailwind-datepicker.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coding-wisely/vue-tailwind-datepicker.git"
  },
  "keywords": [
    "Tailwind",
    "Vue",
    "Datepicker",
    "Vue Datepicker",
    "Tailwind Vue datepicker"
  ],
  "author": "Vladimir Nikolic <nezaboravi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coding-wisely/vue-tailwind-datepicker/issues"
  },
  "homepage": "https://github.com/coding-wisely/vue-tailwind-datepicker#readme",
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "autoprefixer": "^9.7.4",
    "core-js": "^3.6.4",
    "postcss": "^8.2.10",
    "postcss-import": "^12.0.1",
    "rollup": "^1.29.0",
    "rollup-plugin-vue": "^5.1.5",
    "tailwind": "^4.0.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "tailwindcss": "^1.2.0"
  }
}
